<Application x:Class="Mine2CraftWinApp.App"
             Startup="App_OnStartup"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Mine2CraftWinApp"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="MineCraftFont" TargetType="Control">
            <Setter Property="FontFamily" Value="/Fonts/#minecraft"/>
        </Style>
    </Application.Resources>
</Application>